$TTL 2d
$ORIGIN globomantics.co.
@  IN  SOA   ns1.globomantics.co. hostmaster.globomantics.co. (
  2003080800 ; serial number
  12h; refresh
  15m; update retry
  3w; expiry
  2h; minimum
)

; name server RR for the domain
@ IN NS ns1.globomantics.co.

; Zone hosts go here
ns1 IN A 127.0.0.1
@ IN A 172.16.16.16
